# Test Environment Configuration
# Copy this to .env.test.local for local testing with your own database

# PostgreSQL Test Database (use a separate test database)
DATABASE_URL=postgresql+psycopg2://postgres:felicia@localhost:5432/RHOpenLabs_test

# JWT Configuration (test secrets - NEVER use in production)
SECRET_KEY=test-secret-key-change-in-production
ALGORITHM=HS256

# CORS Configuration
CORS_ORIGINS=["http://localhost:3000", "http://localhost:8000"]

# Rate Limiting
RATE_LIMIT_ENABLED=false

# Logging
LOG_LEVEL=debug

# Test Mode
TESTING=true
